{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "false"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-syntax-flow",
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ]
}
